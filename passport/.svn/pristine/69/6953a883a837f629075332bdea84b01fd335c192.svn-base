<?xml version="1.0" encoding="UTF-8"?>
 <!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.easywed.stargate.dao.mapper.AuthMapper">
	<resultMap type="cn.easywed.stargate.dao.persistence.Auth" id="authMap">
		<id column="auth_id" property="authId" />
		<id column="auth_name" property="authName" />
		<id column="description" property="description" />
		<id column="create_time" property="createTime" />
	</resultMap>
	

	
	<insert id="addAuth" useGeneratedKeys="true" keyProperty="auth_id"
		parameterType="cn.easywed.stargate.dao.persistence.Auth">
		INSERT INTO auth(auth_name,description,create_time) 
		VALUES(
		#{authName},
		#{description},
		#{createTime}
		)
	</insert>
	
	<select id="getById" resultMap="authMap" parameterType="long">
		SELECT * FROM auth WHERE 
		auth_id = #{authId}
	</select>
	
</mapper>
